{
  "name": "seveneightfive",
  "short_name": "785 MAG",
  "description": "Topeka Arts + Entertainment",
  "version": "1.0.0",
  "manifest_version": 2, // Or 3 for newer browser extensions, or other values for different platforms
  "icons": [
    {
      "src": "/site-images/785logo_App-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/site-images/785-Splash-512-White.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "site-images/icon16.png",
      "48": "site-images/icon48.png"
    }
  }
}
